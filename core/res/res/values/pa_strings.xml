<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Label for item that reboots the device in device options dialog -->
    <string name="global_action_reboot">Reboot</string>
    <!-- Button to reboot the device, within the reboot options dialog -->
    <string name="reboot_reboot">@string/global_action_reboot</string>
    <!-- Button to reboot the device into recovery, within the reboot options dialog -->
    <string name="reboot_recovery">Recovery</string>
    <!-- Button to reboot the device into fastboot, within the reboot options dialog -->
    <string name="reboot_fastboot">Fastboot</string>
    <!-- Button to reboot the phone into download, within the Reboot Options dialog -->
    <string name="reboot_download">Download</string>

    <!-- Reboot confirmation dialog title -->
    <string name="reboot_system" product="tablet">Reboot tablet</string>
    <string name="reboot_system" product="default">Reboot phone</string>
    <!-- Reboot confirmation dialog.  This is the message shown to the user when he chooses to reboot the device -->
    <string name="reboot_confirm" product="tablet">Your tablet will reboot.</string>
    <string name="reboot_confirm" product="default">Your phone will reboot.</string>

    <string name="global_action_choose_profile">choose profile</string>
    <string name="profile_none">none profile</string>
    <string name="profile_picker_title">pick profile</string>
    <string name="wildcardProfile">1</string>

    <!-- Reboot progress dialog. This is shown if the user chooses to reboot the device. -->
    <string name="reboot_progress">Rebooting\u2026</string>

  <!--From power menu-->
  <java-symbol type="array" name="shutdown_reboot_options" />
  <java-symbol type="array" name="shutdown_reboot_actions" />
  <java-symbol type="array" name="shutdown_reboot_icons" />
  <java-symbol type="drawable" name="ic_lock_reboot" />
  <java-symbol type="drawable" name="ic_lock_reboot_recovery" />
  <java-symbol type="drawable" name="ic_lock_reboot_bootloader" />
  <java-symbol type="drawable" name="ic_lock_screenshot" />
  <java-symbol type="drawable" name="pa_world_logo" />
  <java-symbol type="string" name="global_actions" />
  <java-symbol type="string" name="global_action_reboot" />
  <java-symbol type="string" name="global_action_screenshot" />
  <java-symbol type="string" name="reboot_confirm" />
  <java-symbol type="string" name="reboot_progress" />

  <!--Notification led-->
  <java-symbol type="array" name="notification_light_package_mapping" />

    <!-- label for screen record item in phone options dialog -->
    <string name="global_action_screen_record">Screen record</string>

    <string name="permlab_interceptSmsSent">intercept outgoing SMS</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_interceptSmsSent">Allows the app to
        intercept an outgoing SMS.
        Malicious apps may use this to prevent outgoing SMS messages.</string>
    <!-- Setting confirmation dialog strings. Shown when any UI interaction can be modified by user input -->
    <string name="setting_confirmation_ask_me_later">Ask later</string>
    <string name="setting_confirmation_yes">Yes</string>
    <string name="setting_confirmation_no">No</string>

  <!--Setting confirmation dialog-->
  <java-symbol type="id" name="setting_confirmation_dialog_visual_hint" />
  <java-symbol type="layout" name="setting_confirmation_dialog" />
  <java-symbol type="string" name="setting_confirmation_ask_me_later" />
  <java-symbol type="string" name="setting_confirmation_yes" />
  <java-symbol type="string" name="setting_confirmation_no" />

  <!--Camera flash-->
  <java-symbol type="bool" name="config_device_has_camera_flash" />

    <!-- Title of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permlab_recordScreen">Record content being displayed</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_recordScreen">Allows the app to record the users screen.</string>

    <!-- Safe headset volume warning dialog -->
    <string name="safe_headset_warning_title">Safe headset volume</string>
    <string name="safe_headset_warning_message">Would you like to disable the recommended volume level warning?</string>

  <!-- Safe headset volume warning dialog -->
  <java-symbol type="string" name="safe_headset_warning_title" />
  <java-symbol type="string" name="safe_headset_warning_message" />
  <java-symbol type="drawable" name="safe_headset_volume_warning" />
</resources>
